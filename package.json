{
  "name": "@sagacify/autoroute",
  "version": "1.2.0",
  "description": "Automatic router for simple REST json api",
  "main": "src/Autoroute.js",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "format": "semistandard --fix",
    "test": "mocha --recursive",
    "test:lint": "semistandard --verbose | snazzy",
    "test:cover": "nyc --reporter=text-summary --reporter=html npm test",
    "test:coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test:all": "npm run test:lint && npm run test:cover"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sagacify/autoroute.git"
  },
  "bugs": {
    "url": "https://github.com/Sagacify/autoroute/issues"
  },
  "homepage": "https://github.com/Sagacify/autoroute#readme",
  "keywords": [
    "api",
    "automatic",
    "express",
    "json",
    "rest",
    "router",
    "routing"
  ],
  "author": "@oliviercuyp",
  "contributors": [
    "Fran√ßois Beuvens <francois@sagacify.com>",
    "Emanuel Peroni <emanuel@sagacify.com>",
    "Nicolas Vrielynck <nicolas@sagacify.com>",
    "Quentin Gusbin <quentin@sagacify.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "to-slug-case": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "express": "^4.17.1",
    "mocha": "^9.0.0",
    "nyc": "^15.1.0",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "before",
      "after",
      "beforeEach",
      "afterEach"
    ]
  }
}
